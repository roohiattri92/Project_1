package Array__12;

public class Pring_Frog_Cat_divby_1 {

	public static void main(String[] args) {
		int a=100;
		int b=315;
		int c=90;
		if(a%2==0)
		
		{
			System.out.println("Frog");
		}
		else 
		{
			System.out.println("Roohi");
		}
		if(b%5==0)
		{
			System.out.println("cat");
		}
		else
		{
			System.out.println("Ramanpreet kaur");
		}
		if (a%2==0&&b%5==0)
		{
			System.out.println("cat" + "Frog");
		}
		else 
		{
			System.out.println("nothing is same");
		}
	}

}
