package Pateran_FOR_LOOP;

public class Patern3 {

	public static void main(String[] args) 
	{
		int i, j,f;
		int n=9;
		for(i=1;i<=n;i++)
		{
			
			for(j=1;j<n-i;j++)
			{
				System.out.print(" ");
				
			}
			for(f=1;f<2*i-1;i++)
			{
				System.out.print("*");
			}
			
			System.out.println("");
			
		}

	}

}
