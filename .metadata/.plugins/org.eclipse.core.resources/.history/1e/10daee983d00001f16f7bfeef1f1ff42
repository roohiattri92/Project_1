package selenium_program;

import org.openqa.selenium.By;
import org.openqa.selenium.WebElement;
import org.openqa.selenium.chrome.ChromeDriver;

public class htmlisdisplay_isenable {

	public static void main(String[] args) {
    ChromeDriver p=new ChromeDriver();
    p.get("file:///C:/Users/roohi/Downloads/learningHTML1.html");
   WebElement papa= p.findElement(By.name("lname"));
  boolean r= papa.isDisplayed();
  boolean r1= papa.isEnabled();
  System.out.println(r1);
  System.out.println(r);
	}

}
