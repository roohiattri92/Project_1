package String_12;

public class Palindron_String {

	public static void main(String[] args) 
	{
               String name="MOM";
              System.out.println(name);
            int g=  name.length();
            System.out.println(g);
              
            int i=0;
            int j=name.length()-1;
            System.out.println(j);
	}

}
