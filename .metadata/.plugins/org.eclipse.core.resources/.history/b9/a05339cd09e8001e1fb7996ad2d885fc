		char gender[]=new char[5];
		gender[0]='M';
		gender[1]='M';
		gender[2]='M';
		gender[3]='F';
		gender[4]='M';
        int roll_no[]=new int[5];
        roll_no[0]=1;
        roll_no[1]=2;
        roll_no[2]=3;
        roll_no[3]=4;
        roll_no[4]=5;
       long mobile_no[]=new long[5];
       mobile_no[0]=96857;
       mobile_no[1]=94857;
       mobile_no[2]=90857;
       mobile_no[3]=92857;
       mobile_no[4]=91857;
        System.out.println(student_name.length);
        System.out.println(Arrays.toString(student_name));
        System.out.println(Arrays.toString(gender));
        System.out.println(Arrays.toString(roll_no));
        System.out.println(Arrays.toString(mobile_no));
System.out.println(student_name[0]);
       
    

